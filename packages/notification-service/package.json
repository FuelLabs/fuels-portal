{
  "name": "notification-service",
  "private": true,
  "version": "0.0.0",
  "main": "./src/index.ts",
  "scripts": {
    "build": "pnpm ts:check && tsup --dts",
    "ts:check": "tsc --noEmit",
    "dev:notification": "ts-node-dev -r tsconfig-paths/register --transpile-only --watch ./src/**/*, src/index.ts"
  },
  "dependencies": {
    "express": "^4.18.2",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@fuels-portal/config": "workspace:*",
    "@types/express": "^4.17.17",
    "@types/node": "^18.13.0",
    "tsup": "^7.2.0",
    "typescript": "^5.1.6",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0"
  }
}
